{
    "extension": "RDFTransform",
    "version": "2.2.0",
    "baseIRI": "http://localhost:3333/",
    "namespaces": {
        "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
        "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
        "owl": "http://www.w3.org/2002/07/owl#",
        "xsd": "http://www.w3.org/2001/XMLSchema#",
        "vcard": "http://www.w3.org/2006/vcard/ns#"
    },
    "subjectMappings": [
        {
            "valueSource": {
                "source": "column",
                "columnName": "driverId"
            },
            "expression": {
                "language": "grel",
                "code": "\"driver_\" + value"
            },
            "typeMappings": [
                {
                    "prefix": "",
                    "valueSource": {
                        "source": "constant",
                        "constant": "Driver"
                    }
                }
            ],
            "propertyMappings": [
                {
                    "prefix": "",
                    "valueSource": {
                        "source": "constant",
                        "constant": "code"
                    },
                    "objectMappings": [
                        {
                            "valueType": {
                                "type": "literal"
                            },
                            "valueSource": {
                                "source": "column",
                                "columnName": "code"
                            }
                        }
                    ]
                },
                {
                    "prefix": "",
                    "valueSource": {
                        "source": "constant",
                        "constant": "dateOfBirth"
                    },
                    "objectMappings": [
                        {
                            "valueType": {
                                "type": "datatype_literal",
                                "datatype": {
                                    "prefix": "xsd",
                                    "valueSource": {
                                        "source": "constant",
                                        "constant": "date"
                                    }
                                }
                            },
                            "valueSource": {
                                "source": "column",
                                "columnName": "dob"
                            }
                        }
                    ]
                },
                {
                    "prefix": "",
                    "valueSource": {
                        "source": "constant",
                        "constant": "nationality"
                    },
                    "objectMappings": [
                        {
                            "valueType": {
                                "type": "literal"
                            },
                            "valueSource": {
                                "source": "column",
                                "columnName": "nationality"
                            }
                        }
                    ]
                },
                {
                    "prefix": "",
                    "valueSource": {
                        "source": "constant",
                        "constant": "wikipedia"
                    },
                    "objectMappings": [
                        {
                            "valueType": {
                                "type": "iri"
                            },
                            "valueSource": {
                                "source": "column",
                                "columnName": "url"
                            }
                        }
                    ]
                },
                {
                    "prefix": "",
                    "valueSource": {
                        "source": "constant",
                        "constant": "name"
                    },
                    "objectMappings": [
                        {
                            "valueType": {
                                "type": "bnode"
                            },
                            "valueSource": {
                                "source": "row_index"
                            },
                            "typeMappings": [
                                {
                                    "prefix": "",
                                    "valueSource": {
                                        "source": "constant",
                                        "constant": "DriverName"
                                    }
                                }
                            ],
                            "propertyMappings": [
                                {
                                    "prefix": "",
                                    "valueSource": {
                                        "source": "constant",
                                        "constant": "forename"
                                    },
                                    "objectMappings": [
                                        {
                                            "valueType": {
                                                "type": "literal"
                                            },
                                            "valueSource": {
                                                "source": "column",
                                                "columnName": "forename"
                                            }
                                        }
                                    ]
                                },
                                {
                                    "prefix": "",
                                    "valueSource": {
                                        "source": "constant",
                                        "constant": "surname"
                                    },
                                    "objectMappings": [
                                        {
                                            "valueType": {
                                                "type": "literal"
                                            },
                                            "valueSource": {
                                                "source": "column",
                                                "columnName": "surname"
                                            }
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                }
            ]
